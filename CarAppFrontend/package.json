{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.1",
  "dependencies": {
    "angular": "1.7.x",
    "angular-datatables": "^0.6.2",
    "angular-moment": "^1.3.0",
    "angular-moment-picker": "^0.10.2",
    "angular-route": "1.7.x",
    "angularjs-bootstrap-jquery": "^1215.311.210",
    "angular-auth0": "^3.0.4",
    "auth0-js": "^9.10.4",
    "bootstrap": "4.3.1",
    "datatables.net": "^1.10.19",
    "datatables.net-bs4": "^1.10.19",
    "datatables.net-buttons": "^1.5.6",
    "moment": "^2.24.0",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "angular-mocks": "1.7.x",
    "cpx": "^1.5.0",
    "angular-http-server": "^1.9.0",
    "jasmine-core": "^3.3.0",
    "jquery": "^3.4.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "protractor": "^5.4.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist,jquery/dist,moment,datatables.net*,auth0-js}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "angular-http-server --path ./app -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
}
